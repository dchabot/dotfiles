###SSH Port Forwarding Configuration###

####Global Configuration Options###

#Host * will apply to all hosts
Host *
	#Note default is 3
    ServerAliveCountMax 4
    #Note default is 0
    ServerAliveInterval 15      
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa_air

Host *.slac.stanford.edu
    User dchabot
    ForwardAgent yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes

####Port Forwarding Directives###
Host s3dflogin
    HostName s3dflogin.slac.stanford.edu
    User dchabot
    ForwardAgent yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes

Host centos7
    HostName centos7.slac.stanford.edu
    User dchabot
    ForwardAgent yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes

Host jump
    HostName jump.slac.stanford.edu
    User dchabot
    ForwardAgent yes
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes

Host dev3
    HostName lcls-dev3.slac.stanford.edu
    ProxyJump jump
    User dchabot
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    ForwardAgent yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    #LocalForward 10001 localhost:4000

Host iana
    HostName iana.sdf.slac.stanford.edu
    ProxyJump s3dflogin
    User dchabot
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    ForwardAgent yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    #LocalForward 10001 localhost:4000

Host icd-pc85872 
    HostName icd-pc85872.slac.stanford.edu
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    ProxyJump centos7
    User dchabot
    ForwardAgent yes
    #LocalForward 10001 localhost:4000

Host pc89055
    HostName 134.79.216.85
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    ProxyJump centos7
    User dchabot
    ForwardAgent yes
    #LocalForward 10001 localhost:4000

Host esd-pc80849 
    HostName esd-pc80849.slac.stanford.edu
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    ProxyJump centos7
    User dchabot
    ForwardAgent yes
    #LocalForward 10001 localhost:4000

Host mcclogin
    HostName mcclogin.slac.stanford.edu
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    ProxyJump centos7
    User dchabot
    ForwardAgent yes
    #LocalForward 10001 localhost:4000

Host ldap-test
    HostName ldap-test.slac.stanford.edu
    ProxyJump mcclogin
    User dchabot
    ForwardAgent yes

Host fastx3
    HostName fastx3.slac.stanford.edu
    User dchabot
    ForwardAgent yes

Host xps
    HostName xps.local
    User dc
    ForwardAgent yes

Host cpu-b15-rf02
    Hostname cpu-b15-rf02.slac.stanford.edu
    User laci
    ProxyJump jump
